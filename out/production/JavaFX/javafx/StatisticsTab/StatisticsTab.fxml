<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<Tab fx:id="StatisticsTab" styleClass="tabs" text="Statistics" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.StatisticsTab.StatisticsTabController">
  <content>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
         <children>
            <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <BarChart fx:id="FlowNumberOfRunsChart" title="Flow Number Of Runs" GridPane.rowIndex="1">
                     <xAxis>
                        <CategoryAxis label="Flow" prefHeight="20.0" prefWidth="249.0" side="BOTTOM" />
                     </xAxis>
                     <yAxis>
                        <NumberAxis label="Number Of Runs" prefHeight="126.0" prefWidth="43.0" side="LEFT" upperBound="20.0" />
                     </yAxis>
                  </BarChart>
                  <BarChart fx:id="AverageFlowsRunTimeChart" title="Average Flows Run Time in ms" GridPane.rowIndex="2">
                     <xAxis>
                        <CategoryAxis label="Flow" prefHeight="19.0" prefWidth="222.0" side="BOTTOM" />
                     </xAxis>
                     <yAxis>
                        <NumberAxis label="Average Run Time(ms)" side="LEFT" upperBound="20.0" />
                     </yAxis>
                  </BarChart>
                  <BarChart fx:id="StepNumberOfRunsChart" prefHeight="200.0" prefWidth="312.0" title="Step Number Of Runs" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <xAxis>
                        <CategoryAxis label="Step" prefHeight="19.0" prefWidth="222.0" side="BOTTOM" />
                     </xAxis>
                     <yAxis>
                        <NumberAxis label="Number of Runs" prefHeight="127.0" prefWidth="45.0" side="LEFT" upperBound="20.0" />
                     </yAxis>
                  </BarChart>
                  <BarChart fx:id="AverageStepsRunTimeChart" title="Average Steps Run Time in ms" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <xAxis>
                        <CategoryAxis label="Step" prefHeight="20.0" prefWidth="243.0" side="BOTTOM" />
                     </xAxis>
                     <yAxis>
                        <NumberAxis label="Average Run Time(ms)" prefHeight="126.0" prefWidth="49.0" side="LEFT" upperBound="20.0" />
                     </yAxis>
                  </BarChart>
               </children>
            </GridPane>
         </children></AnchorPane>
  </content>
</Tab>
